@inject IWorkContext WorkContext
@if (!WorkContext.IsComingSoon)
{
    <header>
        <div class="left">
            <div class="logo"><a class="link-overlay" href="/"></a><img title="AQ Booking" src="/images/logo.svg" alt="AQ Booking"></div>
            <ul class="menu">
                <li><a href="#">@R("HOTELS_RESORTS")</a></li>
                <li><a href="#">@R("EXPERIENCES")</a></li>
                <li><a href="#">@R("CURRENT_DEALS")</a></li>
                <li><a href="/Yacht/TravelBlog">@R("TRAVEL_BLOG")</a></li>
            </ul>
        </div>
        <div class="right">
            <ul class="menu">
                <li><a id="btn-login-menu" href="javascript:void(0)">@R("SIGNIN")</a></li>
                <li><a id="btn-register-menu" href="javascript:void(0)">@R("REGISTER")</a></li>
                <li><a href="#">@R("TRIPPLANNER")</a></li>
                <li><a href="#">@R("LISTYOURPRODUCT")</a></li>
                <li><a href="#">@R("HELP")</a></li>
                <li>
                    <a href="javascript:void(0)" id="btn-search-header">
                        <svg width="30" height="30" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M29.8169 28.0494L21.1021 19.3346C22.755 17.2939 23.75 14.6996 23.75 11.875C23.75 5.32718 18.4228 0 11.875 0C5.32712 0 0 5.32718 0 11.875C0 18.4229 5.32718 23.7501 11.875 23.7501C14.6996 23.7501 17.2939 22.755 19.3346 21.1022L28.0494 29.817C28.2935 30.061 28.6891 30.061 28.9333 29.817L29.817 28.9332C30.061 28.6891 30.061 28.2934 29.8169 28.0494ZM11.875 21.2501C6.70537 21.2501 2.50002 17.0447 2.50002 11.875C2.50002 6.70537 6.70537 2.50002 11.875 2.50002C17.0447 2.50002 21.2501 6.70537 21.2501 11.875C21.2501 17.0447 17.0447 21.2501 11.875 21.2501Z" fill="white" />
                        </svg>
                    </a>
                </li>
            </ul>
        </div>
    </header>
}
else
{
    <header class="coming-soon-header">
        <div class="left">
            <div class="logo"><a class="link-overlay" href="@Url.RouteUrl("HomePage")"></a><img src="~/images/logo.svg" alt="AQ logo"></div>
        </div>
        <div class="right">
            <div id="res-menu-icon"><svg class="res-menu-icon" viewBox="0 -53 384 384" xmlns="http://www.w3.org/2000/svg"><path d="m368 154.667969h-352c-8.832031 0-16-7.167969-16-16s7.167969-16 16-16h352c8.832031 0 16 7.167969 16 16s-7.167969 16-16 16zm0 0" /><path d="m368 32h-352c-8.832031 0-16-7.167969-16-16s7.167969-16 16-16h352c8.832031 0 16 7.167969 16 16s-7.167969 16-16 16zm0 0" /><path d="m368 277.332031h-352c-8.832031 0-16-7.167969-16-16s7.167969-16 16-16h352c8.832031 0 16 7.167969 16 16s-7.167969 16-16 16zm0 0" /></svg></div>
            <ul class="menu">
                <li>
                    <a href="https://www.instagram.com/aqbooking/">
                        <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M4.76008 0.0800781C2.17888 0.0800781 0.0800781 2.17888 0.0800781 4.76008V11.2401C0.0800781 13.8213 2.17888 15.9201 4.76008 15.9201H11.2401C13.8213 15.9201 15.9201 13.8213 15.9201 11.2401V4.76008C15.9201 2.17888 13.8213 0.0800781 11.2401 0.0800781H4.76008ZM12.3201 2.96008C12.7161 2.96008 13.0401 3.28408 13.0401 3.68008C13.0401 4.07608 12.7161 4.40008 12.3201 4.40008C11.9241 4.40008 11.6001 4.07608 11.6001 3.68008C11.6001 3.28408 11.9241 2.96008 12.3201 2.96008ZM8.00008 4.04008C10.1853 4.04008 11.9601 5.81488 11.9601 8.00008C11.9601 10.1853 10.1853 11.9601 8.00008 11.9601C5.81488 11.9601 4.04008 10.1853 4.04008 8.00008C4.04008 5.81488 5.81488 4.04008 8.00008 4.04008ZM8.00008 4.76008C6.21448 4.76008 4.76008 6.21448 4.76008 8.00008C4.76008 9.78568 6.21448 11.2401 8.00008 11.2401C9.78568 11.2401 11.2401 9.78568 11.2401 8.00008C11.2401 6.21448 9.78568 4.76008 8.00008 4.76008Z" fill="#414A5A" />
                        </svg>
                    </a>
                </li>
                <li>
                    <a href="https://www.facebook.com/AQbookingdotcom">
                        <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M14.5339 0H1.46615C0.666667 0 0 0.666667 0 1.46615V14.5339C0 15.3333 0.666667 16 1.46615 16H8V9.33333H6V7.33333H8V5.33333C8 3.66667 8.66667 2.66667 10.6667 2.66667H12.6667V4.66667H11.7995C11.2005 4.66667 10.6667 5.20052 10.6667 5.79948V7.33333H13.3333L13 9.33333H10.6667V16H14.5339C15.3333 16 16 15.3333 16 14.5339V1.46615C16 0.666667 15.3333 0 14.5339 0Z" fill="#414A5A" />
                        </svg>
                    </a>
                </li>
                <li><a href="@Url.RouteUrl("HomePage")">@R("TRAVELBLOG")</a></li>
                <li name="@R("SUBSCRIBE_MESSAGE")" class="btn-subscribe-header"></li>
                <li class="lang-top">
                    <div class="dropdown">
                        <button class="dropdown-toggle" id="dropdownMenuButton" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                            @foreach (var item in WorkContext.ListLanguageCommon)
                            {
                                <a id="lang_@item.Id" class="dropdown-item" onclick="aqs.commonFunc.changLanguage(@item.Id,'@item.LanguageCode')" href="#">@item.DisplayName</a>
                            }
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </header>
}